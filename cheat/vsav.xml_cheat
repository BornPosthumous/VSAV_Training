  <cheat desc="Hide Background">
    <script state="on">
      <action>
        temp0=maincpu.pb@FF8033, temp1=maincpu.ow@009452, temp2=maincpu.od@009454
      </action>
    </script>
    <script state="run">
      <action>
        maincpu.pb@FF8033=temp0, maincpu.ow@009452=temp1, maincpu.od@009454=temp2
      </action>
      <action condition="(maincpu.pw@FF8109 != 0 and maincpu.pd@FF810C == 0)"> <!--activate only during match-->
        maincpu.pb@FF8033=0, <!--BG layers-->
        maincpu.pw@FFF000=0, <!--super bars-->
        maincpu.pw@FFF200=0, <!--life bars-->
        maincpu.pw@FF34F6=0, <!--flashing text-->
        maincpu.ow@009452=4E71, maincpu.od@009454=4E714E71 <!--BG sprites-->
      </action>
    </script>
    <script state="off">
      <action>
        maincpu.pb@FF8033=temp0, maincpu.ow@009452=temp1, maincpu.od@009454=temp2
      </action>
    </script>
  </cheat>
  
