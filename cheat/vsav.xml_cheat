  <cheat desc="Recovery"> <comment>Allow for recovery options</comment>
	<parameter>
		<item value="0x01">Towards</item>
		<item value="0x02">Away</item>
		<item value="0x03">Random</item>
	</parameter>‭FF867A‬
	<script state="run"> 
		<action condition="(param == 01)">maincpu.pb@FF880A=01</action>
		<action condition="(param == 02)">maincpu.pb@FF880A=FF</action>
		<action condition="(frame % 60 == 0)">maincpu.pb@FF880A=01</action>
		<action condition="(frame % 84 == 1)">maincpu.pb@FF880A=FF</action>
		<action condition="(frame % 60 == 0)">maincpu.pb@FF840A=01</action>
		<action condition="(frame % 84 == 0)">maincpu.pb@FF840A=FF</action>
	</script>
  </cheat>
  
